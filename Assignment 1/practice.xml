<?xml version="1.0"?>
<!--Create an XML document Employee where it contains the following data :
Employee (Emp_no , name , Department , Manager Name, Project
Assign (Proj_1 , Proj_2 ,Proj_3) , Designation)-->
	<employee>
		<emp_det>
			<emp_no></emp_no>
			<name></name>
			<department></department>
			<manager_name></manager_name>
			<project_assign>
				<proj1></proj1>
				<proj2></proj2>
				<proj3></proj3>
			</project_assign>
			<designation></designation>
		</emp_det>
	</employee>